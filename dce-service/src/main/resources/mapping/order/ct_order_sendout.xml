<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dce.business.dao.order.IOrderSendoutDao">
	<resultMap type="com.dce.business.entity.order.OrderSendoutDo"
		id="orderSendoutResultMap">
		<result property="id" column="id" />
		<result property="orderId" column="order_id" />
		<result property="operatorId" column="operator_id" />
		<result property="address" column="address" />
		<result property="createTime" column="create_time" />
	</resultMap>

	<select id="getById" parameterType="Long" resultMap="orderSendoutResultMap">
		SELECT *
		FROM ct_order_sendout WHERE id = #{id}
	</select>

	<select id="selectOrderSendout" parameterType="Map"
		resultMap="orderSendoutResultMap">
		SELECT *
		FROM ct_order_sendout
		<trim prefix="WHERE" prefixOverrides="AND |OR ">
			<if test="id != null">
				AND id = #{id}
			</if>
			<if test="orderId != null">
				AND order_id = #{orderId}
			</if>
			<if test="operatorId != null">
				AND operator_id = #{operatorId}
			</if>
			<if test="address != null">
				AND address = #{address}
			</if>
			<if test="createTime != null">
				AND create_time = #{createTime}
			</if>
		</trim>

	</select>

	<update id="updateOrderSendoutById" parameterType="com.dce.business.entity.order.OrderSendoutDo">
		UPDATE ct_order_sendout
		<trim prefix="SET" suffixOverrides=",">
			<if test="id != null">
				id = #{id},
			</if>
			<if test="orderId != null">
				order_id = #{orderId},
			</if>
			<if test="operatorId != null">
				operator_id = #{operatorId},
			</if>
			<if test="address != null">
				address = #{address},
			</if>
			<if test="createTime != null">
				create_time = #{createTime},
			</if>
		</trim>
		WHERE id = #{id}
	</update>

	<insert id="addOrderSendout" parameterType="com.dce.business.entity.order.OrderSendoutDo"
		keyProperty="id" useGeneratedKeys="true">
		INSERT INTO ct_order_sendout(
		id
		,order_id
		,operator_id
		,address
		,create_time
		)
		VALUES ( #{id}
		,#{orderId}
		,#{operatorId}
		,#{address}
		,#{createTime}
		)
	</insert>

	<delete id="deleteById" parameterType="Long">
		DELETE FROM
		ct_order_sendout WHERE id = #{id}
	</delete>

</mapper>

